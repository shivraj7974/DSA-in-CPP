**********************Max element in an array ************************************
#include<iostream>
#include<climits>
using namespace std;

int getMax(int num[], int n)
  
  
{
  int maxi=INT_MIN;
    for(int i=0; i<n ; i++)
    {
        maxi=max(maxi,num[i]);
        
    }
    return maxi;
}

int main()
{
  int size;
    cin>>size;
    int num[100];
    for(int i=0; i<size; i++){
        
    
    cin>>num[i];
    }
     cout<<getMax(num,size);
}
