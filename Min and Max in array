 Solution {
  public:
    pair<long long, long long> getMinMax(vector<long long int> arr) {
        // code here
         long min =arr[0];
        long max =arr[0];
        long n = arr.size();
        for(int i=0; i<n; i++){
            if(arr[i]<min){
      class          min = arr[i];
            }
            else if(arr[i]> max){
                max= arr[i];
            }
        }
        return make_pair( min, max);
    }
};
